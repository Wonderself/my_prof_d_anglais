{
  "name": "Gemini Coach Dev",
  "build": { "dockerfile": "Dockerfile" },
  "features": {
    "ghcr.io/devcontainers/features/docker-in-docker:2": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.pylance",
        "esbenp.prettier-vscode",
        "googlecloudtools.cloudcode"
      ]
    }
  },
  "forwardPorts": [8080],
  "postCreateCommand": "bash scripts/bootstrap-dev.sh",
  "remoteUser": "vscode"
}
